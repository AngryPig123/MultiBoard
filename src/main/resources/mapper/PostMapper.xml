<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.study.multiboard.domain.post.mapper.PostMapper">

    <insert id="insertBoard" parameterType="kr.co.study.multiboard.domain.post.dto.request.CreatePostRequest">
        INSERT INTO post (post_seq, title, content)
        VALUES (#{postSeq}, #{title}, #{content})
    </insert>

</mapper>